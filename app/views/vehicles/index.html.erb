<div class="col-lg-12">
<h2>Create Rate for Vehicle:</h2>
  <%= render partial: 'form2'%>
</div>
<div class="col-lg-12">
<h2>Create comming of Vehicle:</h2>
  <%= render partial: 'form'%>
</div>
<div class="col-lg-12">
<h2>Lista de vehiculos</h2>
    <div class="row">
    <div class="col-lg-6">
        <table class="table">
        <thead>
            <th>Id</th>
            <th>Plate</th>
            <th>Rate</th>
            <th>Type vehicle</th>
            <th>Arrive</th>
        </thead>
            <tbody class="vehicle-title">
            <%@vehicles.each do |vehicle|%>
            <tr>
                <td><%=vehicle.id%></td>
                <td><%=link_to vehicle.plate, vehicle_path(vehicle), remote:true%></td>
                <td><%=vehicle.rate.rate%></td>
                <td><%=vehicle.rate.type_vehicle%>
                <td><%=vehicle.created_at%></td>

            </tr>
            <%end%>
            </tbody>
        </table>
    </div>
    <div class="col-lg-6">
    <div class="card">
        
    </div>
  </div>
  </div>
</div>    