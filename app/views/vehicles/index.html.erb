<div class="col-lg-12">
  <%= render partial: 'form'%>
</div>

<h1>Lista de vehiculos</h1>

<div class="row">
  <div class="col-lg-6">
    <table class="table">
      <thead>
          <th>Id</th>
          <th>Plate</th>
          <th>Rate</th>
          <th>Type vehicle</th>
          <th>Arrive</th>
      </thead>
        <tbody class="post-title">
          <%@vehicles.each do |vehicle|%>
          <tr>
            <td><%=vehicle.id%></td>
            <td><%=vehicle.plate%></td>
            <td><%=vehicle.rate.rate%></td>
            <td><%=vehicle.rate.type_vehicle%>
            <td><%=vehicle.created_at%></td>
          </tr>
          <%end%>
        </tbody>
    </table>
  </div>