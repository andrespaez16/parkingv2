<%= form_for @rate, :html =>{:class => "form"}, remote: true do |f| %>
    <div class="form-group">
    <%= f.label :type_vehicle %>:
      <%= f.text_field :type_vehicle, class:"form-control"  %>
    </div>
    <div class="form-group">
    <%= f.label :rate %>:
      <%= f.text_field :rate, class:"form-control" %>
    </div>
    

    <%= f.submit class: "btn btn-primary" %>
  <% end %>